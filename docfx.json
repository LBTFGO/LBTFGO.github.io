{
  "metadata": [
    {
      "src": [
        
          "../src",
          "../images"
        ],
          "files": [
            "**/*.csproj"
          
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}",
          "public/main.css"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "LeBit App Dokumentationen Test",
      "_appTitle": "LeBit App Dokumentationen Test",
      "_enableSearch": true,
      "pdf": true
    }
  }
}